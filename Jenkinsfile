pipeline {
  agent any
  stages {
    stage ('Build') {
	   steps {
	      echo "This is the Build Stage"
		}
	}
	stage ('Test') {
	   steps {
	     echo "This is the test Stage"
		}
	}	 
	stage ('Deploy') {
	   steps {
	     echo "This is the Deploy Stage"
		 
		}
		 
	}
		 
	stage ('Configure') {
	   steps {
	     echo "This is the Configure Stage"
		 
		}
	}
}
}

		 
		 
	